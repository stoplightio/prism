{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs",
    "strict": true,
    "target": "es2018",
    "noEmitHelpers": true,
    "importHelpers": true
  }
}
