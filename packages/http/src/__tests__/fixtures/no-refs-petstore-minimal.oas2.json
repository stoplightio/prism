{
  "info": {
    "version": "2.0",
    "title": "abc"
  },
  "swagger": "2.0",
  "paths": {
    "/pet/findByStatus": {
      "get": {
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "required": true,
            "type": "array",
            "items": {
              "type": "string",
              "enum": ["available", "pending", "sold"],
              "default": "available"
            },
            "collectionFormat": "multi"
          }
        ],
        "responses": {
          "200": {
            "description": "abc",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/pet": {
      "get": {
        "produces": ["application/json"],
        "consumes": ["application/json"],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "required": false,
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int64"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "abc",
            "schema": {
              "type": "string"
            }
          }
        }
      }
    },
    "/pet/login": {
      "get": {
        "tags": ["pet"],
        "summary": "Finds Pets by status",
        "produces": ["application/json", "application/xml"],
        "parameters": [
          {
            "name": "api_key",
            "in": "header",
            "description": "The API Key to perform this operation",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          }
        },
        "security": [

        ]
      }
    }
  },
  "securityDefinitions": {
    "petstore_auth": {
      "type": "oauth2",
      "authorizationUrl": "https://petstore.swagger.io/oauth/dialog",
      "flow": "implicit",
      "scopes": {
        "write:pets": "modify pets in your account",
        "read:pets": "read your pets"
      }
    }
  }
}
