{
  "name": "@stoplight/prism-cli",
  "version": "3.0.0-alpha.9",
  "author": "Stoplight <support@stoplight.io>",
  "bin": {
    "prism": "./bin/run"
  },
  "bugs": "https://github.com/stoplightio/prism/issues",
  "dependencies": {
    "@oclif/command": "^1.0.0",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.0.0",
    "@stoplight/prism-core": "^3.0.0-alpha.9",
    "@stoplight/prism-http-server": "^3.0.0-alpha.9",
    "signale": "^1.4.0",
    "tslib": "^1.0.0",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.0.0",
    "@oclif/tslint": "^3.0.0",
    "@types/chai": "^4.0.0",
    "@types/node": "^12.0.2",
    "@types/signale": "^1.2.1",
    "globby": "^9.0.0",
    "ts-node": "^8.0.0",
    "tsconfig-paths": "^3.0.0",
    "typescript": "^3.0.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "prism",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stoplightio/prism.git"
  },
  "scripts": {
    "prepack": "node -r tsconfig-paths/register ./node_modules/.bin/oclif-dev manifest",
    "version": "node -r tsconfig-paths/register ./node_modules/.bin/oclif-dev readme && git add README.md",
    "cli": "node -r tsconfig-paths/register bin/run",
    "cli:debug": "node --inspect-brk -r tsconfig-paths/register bin/run"
  },
  "types": "lib/index.d.ts"
}
